{
    "version": 2,
    "builds": [
      {
        "src": "picture-flow-service/api.py",
        "use": "@vercel/python"
      },
      {
        "src": "picture-flow-ui/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "picture-flow-service/api.py"
      },
      {
        "src": "/(.*)",
        "dest": "picture-flow-ui/public/index.html"
      }
    ]
  }
  